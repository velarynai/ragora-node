{
  "name": "ragora",
  "version": "0.1.0",
  "description": "Official Node.js SDK for the Ragora RAG API - Build AI-powered knowledge bases",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Ragora",
    "email": "support@ragora.app",
    "url": "https://ragora.app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velarynai/ragora-node.git"
  },
  "bugs": {
    "url": "https://github.com/velarynai/ragora-node/issues"
  },
  "homepage": "https://ragora.app",
  "keywords": [
    "ragora",
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "llm",
    "knowledge-base",
    "vector-search",
    "embeddings",
    "sdk",
    "api",
    "openai",
    "chatgpt"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "example:search": "npx tsx examples/search.ts",
    "example:chat": "npx tsx examples/chat.ts",
    "example:streaming": "npx tsx examples/streaming.ts",
    "example:collections": "npx tsx examples/collections-crud.ts",
    "example:documents": "npx tsx examples/documents.ts",
    "example:marketplace": "npx tsx examples/marketplace.ts",
    "example:credits": "npx tsx examples/credits.ts",
    "example:nextjs": "npx tsx examples/nextjs-integration.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
